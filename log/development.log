  [1m[35mSQL (0.5ms)[0m  INSERT INTO "entries" ("created_at", "feed_id", "guid", "json", "link", "published_at", "title", "updated_at") VALUES ($1, $2, $3, $4, $5, $6, $7, $8) RETURNING "id"  [["created_at", Thu, 20 Nov 2014 19:40:46 UTC +00:00], ["feed_id", 5], ["guid", "http://blog.aweber.com/?p=70300"], ["json", "{\"title\":\"Geek Corner: Our Favorite Festive Dishes Written in Code\",\"link\":\"http://blog.aweber.com/email-marketing/geek-corner-our-favorite-festive-dishes-written-in-code.htm\",\"description\":\"It's no secret that we AWeber folks love geeking out from time to time (Ok, most of the time). We're sharing some of our favorite festive dishes - a la nerd.\",\"content\":\"http://www.aweber.com/about.htm\",\"content_encoded\":\"\\u003Cp\\u003EIt\\u0026#8217;s no secret that we AWeber folks love geeking out from time to time (Ok, most of the time). Since our lunchtime \\u003Ca href=\\\"http://www.aweber.com/about.htm\\\"\\u003Eculture\\u003C/a\\u003E has turned more than a few of us into bona fide foodies, we thought we\\u0026#8217;d share some of our favorite festive dishes to help kickstart your holiday meal planning.\\u003C/p\\u003E\\n\\u003Cp\\u003EOh, and we wrote all the recipes in code. Enjoy!\\u003C/p\\u003E\\n\\u003Ch2\\u003EPumpkin Pie:\\u003C/h2\\u003E\\n\\u003Cp\\u003E\\u003Ca href=\\\"http://blog.aweber.com/wp-content/uploads/2014/10/Screen-Shot-2014-10-31-at-10.28.26-AM.png\\\"\\u003E\\u003Cimg class=\\\"alignnone size-full wp-image-70302\\\" src=\\\"http://blog.aweber.com/wp-content/uploads/2014/10/Screen-Shot-2014-10-31-at-10.28.26-AM.png\\\" alt=\\\"Screen Shot 2014-10-31 at 10.28.26 AM\\\" width=\\\"507\\\" height=\\\"481\\\" /\\u003E\\u003C/a\\u003E\\u003C/p\\u003E\\n\\u003Ch2\\u003EGreen Bean Casserole:\\u003C/h2\\u003E\\n\\u003Cp\\u003E\\u003Ca href=\\\"http://blog.aweber.com/wp-content/uploads/2014/10/Screen-Shot-2014-10-31-at-2.26.52-PM.png\\\"\\u003E\\u003Cimg class=\\\"alignnone size-full wp-image-70303\\\" src=\\\"http://blog.aweber.com/wp-content/uploads/2014/10/Screen-Shot-2014-10-31-at-2.26.52-PM.png\\\" alt=\\\"Screen Shot 2014-10-31 at 2.26.52 PM\\\" width=\\\"640\\\" height=\\\"462\\\" /\\u003E\\u003C/a\\u003E\\u003C/p\\u003E\\n\\u003Ch2\\u003EBread Stuffing:\\u003C/h2\\u003E\\n\\u003Cp\\u003E\\u003Ca href=\\\"http://blog.aweber.com/wp-content/uploads/2014/10/Screen-Shot-2014-10-31-at-2.43.19-PM.png\\\"\\u003E\\u003Cimg class=\\\"alignnone size-full wp-image-70301\\\" src=\\\"http://blog.aweber.com/wp-content/uploads/2014/10/Screen-Shot-2014-10-31-at-2.43.19-PM.png\\\" alt=\\\"Screen Shot 2014-10-31 at 2.43.19 PM\\\" width=\\\"640\\\" height=\\\"409\\\" /\\u003E\\u003C/a\\u003E\\u003C/p\\u003E\\n\\u003Cp\\u003EHungry for more from our AWesome engineers? Check out The \\u003Ca href=\\\"http://engineering.aweber.com/\\\"\\u003EAWeber Engineering blog\\u003C/a\\u003E and sign up to get updates sent straight to your inbox.\\u003C/p\\u003E\",\"comments\":\"http://blog.aweber.com/email-marketing/geek-corner-our-favorite-festive-dishes-written-in-code.htm#comments\",\"pubDate\":\"2014-11-07T07:03:04-08:00\",\"category\":\"Email Marketing\",\"guid\":\"http://blog.aweber.com/?p=70300\",\"dc_creator\":\"AWeber Team\"}"], ["link", "http://blog.aweber.com/email-marketing/geek-corner-our-favorite-festive-dishes-written-in-code.htm"], ["published_at", Fri, 07 Nov 2014 15:03:04 UTC +00:00], ["title", "Geek Corner: Our Favorite Festive Dishes Written in Code"], ["updated_at", Thu, 20 Nov 2014 19:40:46 UTC +00:00]]
