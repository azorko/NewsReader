  [1m[35mSQL (1.3ms)[0m  INSERT INTO "entries" ("created_at", "feed_id", "guid", "json", "link", "published_at", "title", "updated_at") VALUES ($1, $2, $3, $4, $5, $6, $7, $8) RETURNING "id"  [["created_at", Thu, 20 Nov 2014 19:40:46 UTC +00:00], ["feed_id", 5], ["guid", "http://blog.aweber.com/?p=70414"], ["json", "{\"title\":\"12 Days of Holiday Marketing Tips for Small Businesses\",\"link\":\"http://blog.aweber.com/email-marketing/12-days-of-holiday-marketing-tips-for-small-businesses.htm\",\"description\":\"'Tis the season to optimize your holiday marketing efforts.\",\"content\":\"https://nrf.com/media/press-releases/optimism-shines-national-retail-federation-forecasts-holiday-sales-increase-41\",\"content_encoded\":\"\\u003Cp\\u003EThe holiday season is almost upon us! Aside from spreading the yuletide cheer, your customers have only one thing on their minds: shopping.\\u003C/p\\u003E\\n\\u003Cp\\u003EThis time of year marks a lucrative opportunity for small businesses: it\\u0026#8217;s forecasted that consumers will spend \\u003Ca href=\\\"https://nrf.com/media/press-releases/optimism-shines-national-retail-federation-forecasts-holiday-sales-increase-41\\\"\\u003Eover $616.9 billion this season\\u003C/a\\u003E (that\\u0026#8217;s a 4.1 percent increase over holiday 2013 sales).\\u003C/p\\u003E\\n\\u003Cp\\u003ESince we believe it\\u0026#8217;s better to give than to receive, we\\u0026#8217;re gifting you this handy list (don\\u0026#8217;t worry \\u0026#8211; we\\u0026#8217;ve checked it more than twice!) to help you engage your audience and optimize your holiday marketing efforts:\\u003C/p\\u003E\\n\\u003Cp\\u003E\\u003Ca href=https://pinterest.com/pin/create/button/?url=http://blog.aweber.com/email-marketing/12-days-of-holiday-marketing-tips-for-small-businesses.htm?utm_source=share\\u0026#038;utm_medium=social\\u0026#038;utm_content=pinterest\\u0026#038;utm_campaign=sbs14\\u0026#038;media=http://blog.aweber.com/wp-content/uploads/2014/11/infographic-2-01.png\\u0026#038;description=Here%20are%20the%20twelve%20holiday%20marketing%20tips%20your%20small%20business%20needs%20this%20season.%20http://blog.aweber.com/email-marketing/12-days-of-holiday-marketing-tips-for-small-businesses.htm\\u003E\\u003Cimg class=\\\"aligncenter wp-image-70711 size-full\\\" src=\\\"http://blog.aweber.com/wp-content/uploads/2014/11/infographic-2-01.png\\\" alt=\\\"infographic\\\" width=\\\"700\\\" height=\\\"1915\\\" /\\u003E\\u003C/a\\u003E\\u003C/p\\u003E\\n\\u003Cp\\u003E\\u003Ca href=\\\"http://docs.aweber-static.com/pdfs/Small-Business-Saturday-Action-Plan.pdf\\\"\\u003EClick here to download our Small Business Saturday Action Plan checklist\\u003C/a\\u003E.\\u003C/p\\u003E\\n\\u003Cp\\u003EWhat are you doing to give thanks to your customers? Share your seasonal marketing tips with us in the comments.\\u003C/p\\u003E\",\"comments\":\"http://blog.aweber.com/email-marketing/12-days-of-holiday-marketing-tips-for-small-businesses.htm#comments\",\"pubDate\":\"2014-11-19T07:54:50-08:00\",\"category\":\"Email Marketing\",\"guid\":\"http://blog.aweber.com/?p=70414\",\"dc_creator\":\"Olivia Dello Buono\"}"], ["link", "http://blog.aweber.com/email-marketing/12-days-of-holiday-marketing-tips-for-small-businesses.htm"], ["published_at", Wed, 19 Nov 2014 15:54:50 UTC +00:00], ["title", "12 Days of Holiday Marketing Tips for Small Businesses"], ["updated_at", Thu, 20 Nov 2014 19:40:46 UTC +00:00]]
